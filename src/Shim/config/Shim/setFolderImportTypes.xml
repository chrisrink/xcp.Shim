<?xml version="1.0" encoding="UTF-8"?>
<function xmlns="http://documentum.emc.com/2010/UI-Model" id="Shim.setFolderImportTypes" version="1.0.0000.000">
	<name>setFolderImportTypes</name>
	<description>Sets the allowable content types for a given folder type</description>
	<namespace>Shim.functions</namespace>
	<categoryName>String</categoryName>
	<returnType isRepeating="false">STRING</returnType>
	<params>
		<param name="folderName" required="true">
			<type isRepeating="false">STRING</type>
		</param>
		<param name="allowedTypes" required="true">
			<type isRepeating="true">STRING</type>
		</param>
	</params>
</function>